{
  "name": "inventory",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "start": "bun dist/index.html",
    "preview": "vite preview",
    "test:unit": "vitest",
    "build-only": "vite build",
